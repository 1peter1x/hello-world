#include <iostream>
using namespace std;

int main() {
  int x, y, z=1;
  cout << "Enter number and power." <<endl;
  cout << "Enter base number: " <<endl;
  cin >> x;
  cout << "Enter exponent: " <<endl;
  cin>>y;
  for (int i=0; i<y; i++) z=z*x;
  cout << x << " to the power of " << y << " is " << z <<endl;
	return 0;
}
