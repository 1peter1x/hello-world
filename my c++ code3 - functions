int pow(int x, int y){
  if (y==0) return 1;
  else {
    int z=1;
    for (int i=0; i<y; i++) z=z*x;
    return z;
  }
}
double sin(double opp, double hyp){
  double x=opp/hyp;
  return x;
}

double avg(int val){
  int x=0;
  int val[6];
  for (int i=0, i<6, i++){
    x+=val[i];
  }
  z=x/6
  return z;
}

int main(){
  int value[]= {1,2,3,4,5,6};
  w=avg(value);
  cout <<w;
}
